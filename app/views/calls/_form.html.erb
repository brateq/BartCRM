<%= simple_form_for(@call) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :subject %>
    <%= f.input :call_type %>
    <%= f.input :time %>
    <%= f.input :user_id %>
    <%= f.input :company_id %>
    <%= f.input :contact_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>

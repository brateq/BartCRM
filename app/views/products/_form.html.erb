<%= simple_form_for(@product) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :price %>
    <%= f.input :start %>
    <%= f.input :end %>
    <%= f.input :code %>
    <%= f.input :user_id %>
    <%= f.input :description %>
    <%= f.input :stage %>
    <%= f.input :place %>
    <%= f.input :category %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
